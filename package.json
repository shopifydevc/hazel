{
	"name": "hazel-chat",
	"private": true,
	"packageManager": "bun@1.3.3",
	"workspaces": {
		"packages": [
			"apps/*",
			"packages/*"
		],
		"catalogs": {
			"effect": {
				"effect": "^3.19.6",
				"@effect/platform": "0.93.3",
				"@effect/platform-bun": "0.84.0",
				"@effect/platform-browser": "0.73.0",
				"@effect/platform-node": "0.101.1",
				"@effect/rpc": "0.72.2",
				"@effect/experimental": "^0.57.4",
				"@effect/language-service": "^0.55.2",
				"@effect/sql": "0.48.0",
				"@effect/sql-pg": "0.49.6",
				"@effect/cluster": "^0.53.5",
				"@effect/opentelemetry": "0.59.0",
				"@effect/workflow": "0.13.0",
				"@effect-aws/client-s3": "1.10.6",
				"@effect-aws/s3": "0.2.4",
				"@effect-atom/atom": "0.4.1",
				"@effect-atom/atom-react": "0.4.0"
			}
		}
	},
	"scripts": {
		"dev": "turbo dev",
		"build": "turbo build typecheck",
		"start": "turbo start",
		"setup": "turbo setup",
		"typecheck": "turbo build typecheck",
		"format:fix": "biome check --write --unsafe",
		"test": "vitest",
		"test:once": "vitest run",
		"test:debug": "vitest --inspect-brk --no-file-parallelism",
		"test:coverage": "vitest run --coverage --coverage.reporter=text"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.7",
		"@vitest/coverage-v8": "^4.0.13",
		"turbo": "^2.6.1",
		"typescript": "^5.9.3",
		"vitest": "^4.0.15",
		"wrangler": "^4.50.0"
	},
	"trustedDependencies": []
}
