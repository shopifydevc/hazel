{
	"name": "@hazel/backend",
	"module": "src/index.ts",
	"type": "module",
	"private": true,
	"exports": {
		".": "./src/index.ts",
		"./api": "./src/api.ts",
		"./rpc/groups/*": "./src/rpc/groups/*.ts",
		"./rpc/middleware/client": "./src/rpc/middleware/client.ts"
	},
	"scripts": {
		"dev": "bun run --hot src/index.ts",
		"start": "bun run src/index.ts",
		"typecheck": "tsc --noEmit",
		"setup": "bun run scripts/setup.ts",
		"sync:workos": "bun run src/scripts/sync-workos.ts",
		"reset": "bun run scripts/reset-all.ts",
		"reset:dry-run": "bun run scripts/reset-all.ts --dry-run",
		"reset:force": "bun run scripts/reset-all.ts --force"
	},
	"devDependencies": {
		"@effect/language-service": "catalog:effect",
		"@types/bun": "1.3.2",
		"drizzle-kit": "^0.31.7",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"@effect-aws/client-s3": "catalog:effect",
		"@effect-aws/s3": "catalog:effect",
		"@effect/cluster": "catalog:effect",
		"@effect/opentelemetry": "catalog:effect",
		"@effect/platform": "catalog:effect",
		"@effect/platform-bun": "catalog:effect",
		"@effect/platform-node": "catalog:effect",
		"@effect/rpc": "catalog:effect",
		"@effect/sql": "catalog:effect",
		"@effect/sql-pg": "catalog:effect",
		"@hazel/db": "workspace:*",
		"@hazel/domain": "workspace:*",
		"@opentelemetry/sdk-logs": "^0.208.0",
		"@opentelemetry/sdk-metrics": "^2.2.0",
		"@opentelemetry/sdk-trace-node": "^2.2.0",
		"@opentelemetry/sdk-trace-web": "^2.2.0",
		"@workos-inc/node": "^7.73.0",
		"drizzle-orm": "^0.44.7",
		"effect": "catalog:effect",
		"jose": "^6.1.2",
		"metascraper": "^5.49.5",
		"metascraper-description": "^5.49.5",
		"metascraper-image": "^5.49.5",
		"metascraper-logo": "^5.49.5",
		"metascraper-publisher": "^5.49.5",
		"metascraper-title": "^5.49.5",
		"metascraper-url": "^5.49.5",
		"pg": "^8.16.3",
		"react-tweet": "^3.2.2"
	}
}
