# Backend API URL
# Local backend service (Docker)
VITE_BACKEND_URL=http://localhost:3003

# Cluster Service URL
# Local cluster service (Docker)
VITE_CLUSTER_URL=http://localhost:3020

# WorkOS Client ID
# Get this from: https://dashboard.workos.com/
VITE_WORKOS_CLIENT_ID=client_your_client_id_here

# WorkOS Redirect URI
# Frontend callback URL (Vite dev server)
VITE_WORKOS_REDIRECT_URI=http://localhost:5173/auth/callback

# Electric SQL URL
# Local Electric (Docker) - recommended for development
VITE_ELECTRIC_URL=http://localhost:5133/v1/shape

# For Electric Cloud (production), use:
# VITE_ELECTRIC_URL=https://api.electric-sql.cloud/v1/shape?source_id=your_source_id&secret=your_jwt_token

# Cloudflare R2 Public URL
# Your CDN URL for public assets
VITE_R2_PUBLIC_URL=https://cdn.your-domain.com
