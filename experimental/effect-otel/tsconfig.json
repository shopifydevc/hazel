{
	"compilerOptions": {
		"target": "ES2022",
		"lib": ["ES2022"],
		"module": "ESNext",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"noEmit": true,
		"strict": true,
		"skipLibCheck": true,
		"types": ["bun"],
		"plugins": [{ "name": "@effect/language-service" }]
	},
	"include": ["src/**/*.ts"]
}
