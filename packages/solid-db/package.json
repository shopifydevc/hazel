{
  "name": "@tanstack/solid-db",
  "version": "0.2.5",
  "description": "Solid integration for @tanstack/db",
  "author": "Kyle Mathews",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TanStack/db.git",
    "directory": "packages/solid-db"
  },
  "homepage": "https://tanstack.com/db",
  "keywords": [
    "optimistic",
    "solid",
    "typescript"
  ],
  "scripts": {
    "build": "vite build",
